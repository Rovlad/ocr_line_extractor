# Google Cloud Document AI Configuration
# Copy this file to .env and fill in your actual values

# Your Google Cloud Project ID
GOOGLE_CLOUD_PROJECT_ID=your-project-id-here

# Your Document AI Processor ID  
DOCUMENT_AI_PROCESSOR_ID=your-processor-id-here

# Your Google Cloud Service Account JSON (as a single line string)
# Remove all newlines and escape quotes if needed
GOOGLE_APPLICATION_CREDENTIALS_JSON={"type":"service_account","project_id":"your-project","private_key_id":"..."}

# Document AI Location (optional, defaults to 'us')
DOCUMENT_AI_LOCATION=us

# Example of how to format the JSON credential:
# Take your service account JSON file content and put it all on one line
# Example: {"type":"service_account","project_id":"your-project","private_key_id":"abc123","private_key":"-----BEGIN PRIVATE KEY-----\nMIIE...","client_email":"service@project.iam.gserviceaccount.com","client_id":"123","auth_uri":"https://accounts.google.com/o/oauth2/auth","token_uri":"https://oauth2.googleapis.com/token","auth_provider_x509_cert_url":"https://www.googleapis.com/oauth2/v1/certs","client_x509_cert_url":"https://www.googleapis.com/robot/v1/metadata/x509/service%40project.iam.gserviceaccount.com"} 